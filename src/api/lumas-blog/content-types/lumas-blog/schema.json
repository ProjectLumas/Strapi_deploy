{
  "kind": "collectionType",
  "collectionName": "lumas_blogs",
  "info": {
    "singularName": "lumas-blog",
    "pluralName": "lumas-blogs",
    "displayName": "Lumas Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Content": {
      "type": "blocks"
    },
    "Slug": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "lumas_blogs"
    }
  }
}
